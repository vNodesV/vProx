# Optional geo DB paths
# Preferred IP2Location MMDB
IP2LOCATION_MMDB=

# GeoLite2 fallback DBs
GEOLITE2_COUNTRY_DB=
GEOLITE2_ASN_DB=

# Backup automation
# Enable backups and set either interval (days) and/or max size (bytes)
VPROX_BACKUP_ENABLED=false
VPROX_BACKUP_INTERVAL_DAYS=0
VPROX_BACKUP_MAX_BYTES=0
VPROX_BACKUP_CHECK_MINUTES=10

# Rate limiting
VPROX_RPS=25
VPROX_BURST=100
VPROX_AUTO_ENABLED=true
VPROX_AUTO_THRESHOLD=120
VPROX_AUTO_WINDOW_SEC=10
VPROX_AUTO_RPS=1
VPROX_AUTO_BURST=1
VPROX_AUTO_TTL_SEC=900

# Server
VPROX_ADDR=:3000
